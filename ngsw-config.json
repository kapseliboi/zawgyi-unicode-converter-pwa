{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app-shell",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/manifest.json",
          "/assets/appicons/favicon.ico",
          "/*.css",
          "/*.js",
          "/assets/fonts/**"
        ]
      }
    },
    {
      "name": "assets-lazy",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/appicons/**",
          "/assets/images/**",
          "!/assets/images/uploads/**"
        ]
      }
    }
  ]
}
